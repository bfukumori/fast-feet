{
  "name": "fast-feet",
  "version": "0.0.1",
  "description": "Final project for the NestJS formation by Rocketseat.",
  "author": "Bruno Fukumori <brunofukumori@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "pnpm biome check --write src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --no-file-parallelism",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.ts",
    "test:e2e:cov": "vitest run --coverage --config ./vitest.config.e2e.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/static": "^8.1.1",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/platform-fastify": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.8.0",
    "bcrypt-ts": "^7.0.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "nestjs-zod": "^4.3.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.24",
    "@types/node": "^22.15.18",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.1.3",
    "globals": "^16.1.0",
    "prisma": "^6.8.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "unplugin-swc": "^1.5.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@10.11.0"
}
